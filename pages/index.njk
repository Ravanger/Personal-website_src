---
title: Projects
---
<div id="projectslist">
    {% for project in collections.projects %}
        <pre>
            <h2><a href="{{ project.url | url }}">{{ project.data.title }}</a></h2>
            <em>{{ project.date | readableDate }}</em>
            <em>{{ project.data.tags }}</em>
        </pre>
    {% endfor %}
</div>
